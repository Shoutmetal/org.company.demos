    <UserName>$orgcompanyservice</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DemoDbContext" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:m7t8jg2sjn.database.windows.net,1433;Initial Catalog=DbDemo;Persist Security Info=True;User ID=shoutmetal;Password=Pass@word1" Name="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=.;Initial Catalog=DbDemo;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\DemoDbContext_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="data source=.; initial catalog=DbDemo;integrated security=True;MultipleActiveResultSets=True;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
